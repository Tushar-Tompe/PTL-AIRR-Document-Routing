<?xml version="1.0"?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="Maxum.EDM.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <connectionStrings>
        <add name="ServiceModelEx.Properties.Settings.WCFLogbookConnectionString"
            connectionString="Data Source=okc-docld;Initial Catalog=ExceptionLogService;Integrated Security=True;Application Name=SirmDocumentRouter"
            providerName="System.Data.SqlClient" />
        <add name="Maxum.EDM.Properties.Settings.CommonConnectionString"
            connectionString="Data Source=okc-docld;Initial Catalog=common;Integrated Security=True;MultipleActiveResultSets=True;Application Name=SirmDocumentRouter"
            providerName="System.Data.SqlClient" />
        <add name="Maxum.EDM.Properties.Settings.ConnectionString" connectionString="Data Source=okc-docld;Integrated Security=True;MultipleActiveResultSets=True;Application Name=SirmDocumentRouter"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
  
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="LogbookBinding" closeTimeout="00:10:00" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00" transactionFlow="false" transferMode="Buffered" transactionProtocol="OleTransactions" hostNameComparisonMode="StrongWildcard" listenBacklog="10" maxBufferPoolSize="4194304" maxBufferSize="4194304" maxConnections="100" maxReceivedMessageSize="4194304">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384" maxBytesPerRead="4096" maxNameTableCharCount="16384"/>
          <reliableSession ordered="true" inactivityTimeout="00:10:00" enabled="false"/>
          <security mode="Transport">
            <transport clientCredentialType="Windows" protectionLevel="EncryptAndSign"/>
            <message clientCredentialType="Windows"/>
          </security>
        </binding>
      </netTcpBinding>


      <netMsmqBinding>
        <binding name="NetMsmqBinding_IChildImageValidation">
          <security mode="None" />
        </binding>
        <binding name="NetMsmqBinding_IParentImageValidation">
          <security mode="None" />
        </binding>
      </netMsmqBinding>
      <netTcpBinding>
        <binding name="NetTcpBinding_IValidationData" />
      </netTcpBinding>
      
    </bindings>
      <client>
        <endpoint address="net.msmq://spis390-0/airr_child_message"
          binding="netMsmqBinding" bindingConfiguration="NetMsmqBinding_IChildImageValidation"
          contract="ChildQueue.IChildImageValidation"
          name="NetMsmqBinding_IChildImageValidation" />
        
        <endpoint address="net.msmq://spis390-0/airr_parent_message"
                  binding="netMsmqBinding" bindingConfiguration="NetMsmqBinding_IParentImageValidation"
                  contract="ParentQueue.IParentImageValidation"
                  name="NetMsmqBinding_IParentImageValidation" />
        
        <endpoint address="net.tcp://localhost:9030/ValidationManager"
                  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IValidationData"
                  contract="ValidationManager.IValidationData"
                  name="NetTcpBinding_IValidationData">
          <identity>
            <userPrincipalName value="" />
          </identity>
        </endpoint>
        
        
        <!--This endpoint is used to log errors to the Log Book. The Client must reference ServiceModelEx.dll-->
        <!--There will also be a database connection string associated with the endpoint-->
        <endpoint name="LogbookTCP"
                  address="net.tcp://okc-docld:9010/LogbookManager"
                  binding="netTcpBinding" bindingConfiguration="LogbookBinding"
                  contract="ILogbookManager">
          <identity>
            <userPrincipalName value=""/>
          </identity>
        </endpoint>
    </client>
  </system.serviceModel>
  
    <applicationSettings>
        <Maxum.EDM.Properties.Settings>
            <setting name="MaxUnknownFiles" serializeAs="String">
                <value>200</value>
            </setting>
            <setting name="Auth_DL_User" serializeAs="String">
                <value>AutomationAdmin</value>
            </setting>
            <setting name="Auth_DL_PW" serializeAs="String">
                <value>1spiautoadmin</value>
            </setting>
            <setting name="Auth_DL_Server" serializeAs="String">
                <value>okc-docld</value>
            </setting>
            <setting name="TimerIntervalInMinutes" serializeAs="String">
                <value>0.25</value>
            </setting>
            <setting name="Auth_DL_DB" serializeAs="String">
                <value>Doclink2</value>
            </setting>
            <setting name="ValidationDirectory" serializeAs="String">
                <value>\\MSSPTCDLINK2\SIRM_Queue\Completed\Validation</value>
            </setting>
            <setting name="DoclinkEndpoint" serializeAs="String">
                <value>tcp://okc-docld:5555/doclinkServer.soap</value>
            </setting>
            <setting name="DefalutUnknownFolder" serializeAs="String">
                <value>\\MSSPTCDLINK2\Sirm_Unknown</value>
            </setting>
            <setting name="QueueFolder" serializeAs="String">
                <value>C:\Test\Q</value>
            </setting>
        </Maxum.EDM.Properties.Settings>
    </applicationSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
  
</configuration>
